<div class="event">
	<div style="float:left;">
		<div class="event-left"> 
			<img class="event-image" src="{{viewData.imageUrl || '/images/MadeInOK100x100.png'}}">								
		</div>
		<div class="event-right">
			<h3>{{viewData.title}}</h3>
			<h4>{{viewData.date | date:"EEE MMM dd"}}</h4>
			<span class="label label-info">{{viewData.date | date:"h:mm a"}}</span>
		</div>
	</div>
	<div class="event-description">
		{{viewData.description}}
	</div>
</div>